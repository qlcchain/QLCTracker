{
  "name": "qlctracker",
  "version": "v1.3.3",
  "main": "desktop/main.js",
  "author": "QLC Chain Team <admin@qlink.mobi>",
  "description": "QLC Wallet and Explorer",
  "homepage": "https://explorer.qlcchain.org",
  "license": "MIT",
  "scripts": {
    "postinstall": "node patch.js",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "qlc:dev": "ng serve",
    "qlc:dev-testnet": "ng serve --configuration=testnet",
    "qlc:build-testnet": "ng build --configuration=testnet",
    "qlc:build": "ng build --configuration=production",
    "qlc:build-desktop": "ng build --configuration=desktop --base-href ./",
    "qlc:build-desktop-high-mem": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=desktop --base-href ./",
    "qlc:build-desktop-testnet": "ng build --configuration=desktop-testnet --base-href ./",
    "desktop:dev": "electron desktop/main.js",
    "desktop:local": "electron-builder",
    "desktop:devLocal": "electron-builder -c.compression=store",
    "desktop:full": "electron-builder -wml --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@cityofzion/neon-js": "^4.8.0",
    "@ctrl/ngx-chartjs": "^2.0.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/jest": "^24.9.1",
    "@types/jquery": "^3.5.0",
    "@types/mocha": "^5.2.7",
    "bignumber.js": "^8.1.1",
    "bip39": "^3.0.2",
    "blakejs": "^1.1.0",
    "bootstrap": "^4.5.0",
    "chainx.js": "^2.22.32",
    "chart.js": "^2.9.3",
    "chmod-plus": "0.0.3",
    "core-js": "^2.6.11",
    "cross-spawn-with-kill": "^1.0.0",
    "crypto-js": "^3.3.0",
    "electron-download-manager": "^2.1.2",
    "electron-log": "^3.0.9",
    "electron-updater": "^4.3.1",
    "electron-util": "^0.11.0",
    "find-process": "^1.4.3",
    "fs-extra": "^8.1.0",
    "inly": "^4.0.5",
    "jquery": "^3.5.1",
    "js-base64": "^2.6.3",
    "ngx-bootstrap": "^4.3.0",
    "ngx-clipboard": "^12.3.1",
    "ngx-device-detector": "^1.4.5",
    "ngx-logger": "^3.4.6",
    "pidusage": "^2.0.21",
    "ps-node": "^0.1.6",
    "qlc.js": "0.0.7",
    "qrcode": "^1.4.4",
    "rxjs": "^6.6.0",
    "signal-exit": "^3.0.3",
    "to-executable-name": "^2.0.0",
    "tslib": "^1.13.0",
    "uuid": "^3.4.0",
    "xhr2": "^0.1.4",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.51",
    "codelyzer": "^5.2.2",
    "download": "^7.1.0",
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "popper.js": "^1.16.1",
    "protractor": "^7.0.0",
    "tar": "^4.4.13",
    "ts-node": "^8.10.2",
    "tslint": "~5.16.0",
    "typescript": "3.7.5",
    "kind-of": ">=6.0.3",
    "lodash": ">=4.17.19"
  },
  "build": {
    "appId": "org.qlcchain.qlctracker",
    "buildVersion": "1.3.3",
    "productName": "QLCTracker",
    "asar": true,
    "files": [
      "build/icon.ico",
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "desktop/main.js"
    ],
    "directories": {
      "output": "dist-desktop"
    },
    "dmg": {
      "window": {
        "width": 540,
        "height": 380
      },
      "contents": [
        {
          "x": 410,
          "y": 230,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 230,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "extraResources": [],
      "binaries": [],
      "category": "public.app-category.finance"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "extraResources": []
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "desktop": {
        "Categories": "P2P;Finance;Security"
      },
      "extraResources": []
    },
    "publish": [
      {
        "provider": "github",
        "owner": "qlcchain",
        "repo": "QLCTracker"
      }
    ]
  }
}
